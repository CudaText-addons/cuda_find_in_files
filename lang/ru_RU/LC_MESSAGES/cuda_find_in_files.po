#: cd_fi_in_fi.py:832
msgid ""
"\n"
"┌────────────────────────────┬──────────────┬────────────────────────────┬───────────────────────────────────────────┐\n"
"│          Command           │    Hotkey    │           Trick            "
"│                  Comment                  │\n"
"╞════════════════════════════╪══════════════╪════════════════════════════╪═══════════════════════════════════════════╡\n"
"│ Find                       │        Enter │                            │ "
"If focus not in Results/Source            │\n"
"│ Count matches              │        Alt+T │                            "
"│                                           │\n"
"│ Find filenames             │       Ctrl+T │                            "
"│                                           │\n"
"├────────────────────────────┼──────────────┼────────────────────────────┼───────────────────────────────────────────┤\n"
"│ Focus to Results           │   Ctrl+Enter │                            │ "
"If focus not in Results/Source            │\n"
"│ Open found fragment        │        Enter │                            │ "
"If focus in Results. Selects the fragment │\n"
"│ Close and go to found      │   Ctrl+Enter │                            │ "
"If focus in Results. Selects the fragment │\n"
"│ Close and go to found      │   Ctrl+Enter │                            │ "
"If focus in Source. Restores selection    │\n"
"│ Fold Results branch        │       Ctrl+= │                            │ "
"If focus in Results                       │\n"
"├────────────────────────────┼──────────────┼────────────────────────────┼───────────────────────────────────────────┤\n"
"│ In current file            │ Ctrl+Shift+C │      Shift+Сlick \"Current\" "
"│                                           │\n"
"│ In all tabs                │              │ Ctrl      +Сlick \"Current\" "
"│                                           │\n"
"│ In current tab             │              │ Ctrl+Shift+Сlick \"Current\" "
"│                                           │\n"
"│ Choose file                │       Ctrl+B │      Shift+Сlick  \"Browse\" "
"│                                           │\n"
"│ Depth \"Only\"               │        Alt+Y │                  Ctrl+Num0 "
"│                                           │\n"
"│ Depth \"+1 level\"           │        Alt+! │                  Ctrl+Num1 "
"│                                           │\n"
"│ Depth \"All\"                │        Alt+L │                  Ctrl+Num9 "
"│                                           │\n"
"├────────────────────────────┼──────────────┼────────────────────────────┼───────────────────────────────────────────┤\n"
"│ Save values as preset      │       Ctrl+S │                            "
"│                                           │\n"
"│ Presets dialog             │   Ctrl+Alt+S │                            "
"│                                           │\n"
"│ Use Preset #1(..#5)        │  Ctrl+1(..5) │                            │ "
"Others from menu/dialog                   │\n"
"│ Use Layout #1(..#5)        │   Alt+1(..5) │                            │ "
"Others from menu                          │\n"
"├────────────────────────────┼──────────────┼────────────────────────────┼───────────────────────────────────────────┤\n"
"│ ±\"Not in\"/±\"Replace\"       │        Alt+V │                            "
"│ 4-states loop to show/hide                │\n"
"│ Engine options             │       Ctrl+E │                            "
"│                                           │\n"
"│ Configure vert. alignments │              │             Ctrl+Сlick \"=\" "
"│                                           │\n"
"├────────────────────────────┼──────────────┼────────────────────────────┼───────────────────────────────────────────┤\n"
"│ Call CudaText's \"Find\"     │       Ctrl+F │                            │ "
"With transfer patern/.*/aA/\"w\"            │\n"
"│ Call CudaText's \"Replace\"  │       Ctrl+R │                            │ "
"With transfer paterns/.*/aA/\"w\"           │\n"
"│ Dialog \"Help\"              │        Alt+H │                            "
"│                                           │\n"
"└────────────────────────────┴──────────────┴────────────────────────────┴───────────────────────────────────────────┘\n"
msgstr ""
"\n"
"┌───────────────────────────────┬──────────────┬────────────────────────────┬───────────────────────────────────────────┐\n"
"│          Команда              │    Хоткей    │           Трюк             "
"│                  Комментарий              │\n"
"╞═══════════════════════════════╪══════════════╪════════════════════════════╪═══════════════════════════════════════════╡\n"
"│ Искать                        │        Enter │                            "
"│ Если фокус не в Результатах/Исходнике     │\n"
"│ Посчитать фрагменты           │        Alt+T │                            "
"│                                           │\n"
"│ Найти имена файлов            │       Ctrl+T │                            "
"│                                           │\n"
"├───────────────────────────────┼──────────────┼────────────────────────────┼───────────────────────────────────────────┤\n"
"│ Активировать Результаты       │   Ctrl+Enter │                            "
"│ Если фокус не в Результатах/Исходнике     │\n"
"│ Перейти к фрагменту           │        Enter │                            "
"│ Если фокус в Результатах                  │\n"
"│ Закрыть и перейти к фрагменту │   Ctrl+Enter │                            "
"│ Если фокус в Результатах                  │\n"
"│ Закрыть и перейти к фрагменту │   Ctrl+Enter │                            "
"│ Если фокус в Исходнике                    │\n"
"│ Свернуть ветку Результатов    │       Ctrl+= │                            "
"│ Если фокус в Результатах                  │\n"
"├───────────────────────────────┼──────────────┼────────────────────────────┼───────────────────────────────────────────┤\n"
"│ В текущем файле               │ Ctrl+Shift+C │      Shift+Сlick \"Текущий"
"\" │                                           │\n"
"│ Во всех табах                 │              │ Ctrl      +Сlick \"Текущий"
"\" │                                           │\n"
"│ В текущем табе                │              │ Ctrl+Shift+Сlick \"Текущий"
"\" │                                           │\n"
"│ Выбрать файл                  │       Ctrl+B │      Shift+Сlick  \"Обзор"
"\"  │                                           │\n"
"│ Глубина \"Тек.папка\"           │        Alt+Y │                  Ctrl"
"+Num0 │                                           │\n"
"│ Глубина \"+1 уровень\"          │        Alt+! │                  Ctrl"
"+Num1 │                                           │\n"
"│ Глубина \"Все\"                 │        Alt+L │                  Ctrl"
"+Num9 │                                           │\n"
"├───────────────────────────────┼──────────────┼────────────────────────────┼───────────────────────────────────────────┤\n"
"│ Сохранить как пресет          │       Ctrl+S │                            "
"│                                           │\n"
"│ Диалог Пресеты                │   Ctrl+Alt+S │                            "
"│                                           │\n"
"│ Прим. Пресет #1(..#5)         │  Ctrl+1(..5) │                            "
"│ Остальные доступны из меню/диалога        │\n"
